{
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.formatOnPaste": true,
  "workbench.colorTheme": "Atom Material Theme",
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.normal": ["#373b41", "#ffffff"],
  "vim.statusBarColors.insert": ["#9955ee", "#263238"],
  "vim.surround": true,
  "workbench.colorCustomizations": {
    "statusBar.background": "#5f00af",
    "statusBar.noFolderBackground": "#5f00af",
    "statusBar.debuggingBackground": "#5f00af",
    "statusBar.foreground": "#ffffff",
    "terminal.ansiBlack": "#263238",
    "terminal.ansiRed": "#e54b4b",
    "terminal.ansiGreen": "#9ece58",
    "terminal.ansiYellow": "#faed70",
    "terminal.ansiBlue": "#396fe2",
    "terminal.ansiMagenta": "#bb80b3",
    "terminal.ansiCyan": "#2ddafd",
    "terminal.ansiWhite": "#d0d0d0",
    "terminal.ansiBrightBlack": "#555864",
    "terminal.ansiBrightRed": "#ff5370",
    "terminal.ansiBrightGreen": "#c3e88d",
    "terminal.ansiBrightYellow": "#ffcb6b",
    "terminal.ansiBrightBlue": "#82aaff",
    "terminal.ansiBrightMagenta": "#c792ea",
    "terminal.ansiBrightCyan": "#89ddff",
    "terminal.ansiBrightWhite": "#ffffff",
    "terminal.background": "#263238",
    "terminal.foreground": "#eceff1",
    "statusBar.debuggingForeground": "#ffffff"
  },
  "settingsSync.ignoredSettings": [
    "editor.fontSize",
    "terminal.integrated.fontSize"
  ],
  "editor.fontLigatures": true,
  "editor.fontFamily": "delugia nerd font",
  "editor.fontSize": 17,
  "terminal.integrated.fontSize": 16,
  "editor.tabSize": 2,
  "editor.minimap.enabled": false,
  "workbench.tree.indent": 18,
  "files.autoSave": "onFocusChange",
  "editor.formatOnSave": true,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 80,
  "[markdown]": {
    "editor.wordBasedSuggestions": false,
    "editor.quickSuggestions": false,
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80,
    "editor.lineNumbers": "off",
    "editor.renderIndentGuides": false
  },
  "[plaintext]": {
    "editor.wordBasedSuggestions": false,
    "editor.quickSuggestions": false,
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80,
    "editor.lineNumbers": "off",
    "editor.renderIndentGuides": false,
    "editor.autoIndent": "none",
    "editor.wrappingIndent": "none"
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "java.errors.incompleteClasspath.severity": "ignore",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "terminal.integrated.shell.linux": "/bin/zsh",
  "go.formatTool": "goimports",
  "editor.rulers": [80],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features",
    "editor.formatOnSave": true,
    "editor.wordWrap": "wordWrapColumn",
    "editor.wrappingIndent": "same",
    "editor.wordWrapColumn": 80
  },
  "html.format.wrapAttributes": "force-expand-multiline",
  "html.format.wrapLineLength": 80,
  "editor.lineDecorationsWidth": 0,
  "editor.glyphMargin": false,
  "workbench.iconTheme": "material-icon-theme",
  "vim.handleKeys": {
    "<C-w>": false
  },
  "workbench.startupEditor": "newUntitledFile",
  "workbench.list.horizontalScrolling": true,
  "customizeUI.titleBar": "inline",
  "angular.experimental-ivy": true,
  "window.titleBarStyle": "native",
  "editor.scrollBeyondLastColumn": 0,
  "rewrap.autoWrap.enabled": true,
  "cSpell.userWords": ["hesparia"],
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "vim.leader": ""
}
